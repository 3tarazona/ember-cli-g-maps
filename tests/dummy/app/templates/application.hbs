<div class="grid-frame vertical -scroll-y">
  <div class="primary title-bar title-bar--dark">
    <span class="title left">
      {{#link-to 'index' 
        tagName="h2"
        class="-white-color -cursor-pointer -nmb"}}
        Ember-CLI GMaps
        <h6 class="gmaps-slogan -white-color">
          For Map Driven Apps {{isMobileNavActive}}
        </h6>
      {{/link-to}}
    </span>
  </div>

  <nav class="primary title-bar th-google-navigation -np">
    <ul class="th-google-tabs">
      <li class="th-google-tabs__mobile-nav">
        <button 
          class="th-google-button {{if isMobileNavActive 'active'}}"
          {{action "toggleMobileNav"}}>Naviation</button>
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'setup.index'}}Setup{{/link-to}}
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'basicUsage.index'}}Basic{{/link-to}}
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'service.index'}}Service{{/link-to}}
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'selections.index'}}Selections{{/link-to}}
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'heatmap.index'}}Heatmap{{/link-to}}
      </li>
      <li class="th-google-tabs__tab">
        {{#link-to 'performance.index'}}Performance{{/link-to}}
      </li>
    </ul>
  </nav>

  <a href="https://github.com/matt-jensen/ember-cli-g-maps">
    <img style="position: absolute; top: 0; right: 0; border: 0; z-index: 10; width: 132px;" src="https://camo.githubusercontent.com/e7bbb0521b397edbd5fe43e7f760759336b5e05f/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f677265656e5f3030373230302e706e67" alt="Fork me on GitHub" data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_green_007200.png">
  </a>

  {{outlet}}
</div>